public class Main {
    public static void main(String[] args) {

        int n = 5;
        int[][] a = new int[n][n];

        for (int i = 0; i < n; i++)
            for (int j = 0; j <= i; j++)
                a[i][j] = j + 1;

        for (int i = 0; i < n; i++) {
            for (int j = 0; j <= i; j++)
                System.out.print(a[i][j] + " ");
            System.out.println();
        }

        System.out.println();

        for (int i = n - 1; i >= 0; i--) {
            for (int j = 0; j <= i; j++)
                System.out.print(a[i][j] + " ");
            System.out.println();
        }
    }
}
