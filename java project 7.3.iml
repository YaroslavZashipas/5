import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int[][] a = new int[2][2];

        for (int i = 0; i < 2; i++)
            for (int j = 0; j < 2; j++)
                a[i][j] = sc.nextInt();

        int det = a[0][0] * a[1][1] - a[0][1] * a[1][0];

        System.out.println(det);
        sc.close();
    }
}
